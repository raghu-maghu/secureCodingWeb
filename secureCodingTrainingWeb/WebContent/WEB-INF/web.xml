<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>secureCodingTrainingWeb</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>tmpFile.dir</param-name>
    <param-value>C:\\Temp</param-value>
  </context-param>
  
  <filter>
                <filter-name>ESAPIFilter</filter-name>
                <filter-class>
                        org.owasp.esapi.filters.ESAPIFilter
                </filter-class>
        </filter>
        <filter-mapping>
                <filter-name>ESAPIFilter</filter-name>
                <url-pattern>/AccountTransfer.svl</url-pattern>
        </filter-mapping>
  <servlet>
    <description></description>
    <display-name>AccountTransfer</display-name>
    <servlet-name>AccountTransfer</servlet-name>
    <servlet-class>test.accounttransfer.AccountTransfer</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>AccountTransfer</servlet-name>
    <url-pattern>/AccountTransfer.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DBConnection</display-name>
    <servlet-name>DBConnection</servlet-name>
    <servlet-class>test.accounttransfer.DBConnection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DBConnection</servlet-name>
    <url-pattern>/DBConnection.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>XmlAttacks</display-name>
    <servlet-name>XmlAttacks</servlet-name>
    <servlet-class>xmlattacks.XmlAttacks</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>XmlAttacks</servlet-name>
    <url-pattern>/XmlAttacks.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>LdapLogin</display-name>
    <servlet-name>LdapLogin</servlet-name>
    <servlet-class>com.ldap.LdapLogin</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LdapLogin</servlet-name>
    <url-pattern>/LdapLogin.svl</url-pattern>
  </servlet-mapping>
  <error-page>
    <error-code>404</error-code>
    <location>/Common.jsp</location>
  </error-page>
  <servlet>
    <description></description>
    <display-name>UploadDownloadFileServlet</display-name>
    <servlet-name>UploadDownloadFileServlet</servlet-name>
    <servlet-class>com.fileupload.UploadDownloadFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UploadDownloadFileServlet</servlet-name>
    <url-pattern>/UploadDownloadFileServlet.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CommandInjection</display-name>
    <servlet-name>CommandInjection</servlet-name>
    <servlet-class>com.commandinjection.CommandInjection</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CommandInjection</servlet-name>
    <url-pattern>/CommandInjection.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServletVariable</display-name>
    <servlet-name>ServletVariable</servlet-name>
    <servlet-class>com.racecondition.ServletVariable</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletVariable</servlet-name>
    <url-pattern>/ServletVariable.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>DownloadFileServlet</display-name>
    <servlet-name>DownloadFileServlet</servlet-name>
    <servlet-class>com.filedownload.DownloadFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DownloadFileServlet</servlet-name>
    <url-pattern>/DownloadFileServlet.svl</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>CaptchaLoginServlet</display-name>
    <servlet-name>CaptchaLoginServlet</servlet-name>
    <servlet-class>com.captcha.CaptchaLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CaptchaLoginServlet</servlet-name>
    <url-pattern>/CaptchaLoginServlet.svl</url-pattern>
  </servlet-mapping>
</web-app>